{
	"for": {
		"prefix": ["for"],
		"body": [
			"for(int i=${1:start};i<=${2:counter};i++)",
			"{",
			"\t${0:contine}",
			"}"
		]
	},
	"Class": {
    "prefix": ["class"],
    "body": [
      "/**",
      " * @author ${VIM:\\$USER}",
      " */",
